        "parameters": {
            "type": "object",
            "properties": {
                "target_user_id": {
                    "type": "integer",
                    "description": "要点赞的目标QQ号"
                },
                "times": {
                    "type": "integer",
                    "description": "点赞次数（默认10次）",
                    "default": 10
                }
            },
            "required": ["target_user_id"]
        }
    }
}
