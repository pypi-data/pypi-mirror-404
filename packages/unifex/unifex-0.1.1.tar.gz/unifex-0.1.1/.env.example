# Azure Document Intelligence credentials
# Copy this file to .env and fill in your values
UNIFEX_AZURE_DI_ENDPOINT=https://YOUR_RESOURCE_NAME.cognitiveservices.azure.com
UNIFEX_AZURE_DI_KEY=YOUR_API_KEY
UNIFEX_AZURE_DI_MODEL=prebuilt-read

# Google Document AI credentials
# Processor name format: projects/{project}/locations/{region}/processors/{processor_id}
# Region examples: us, eu, asia
UNIFEX_GOOGLE_DOCAI_PROCESSOR_NAME=projects/YOUR_PROJECT_ID/locations/YOUR_REGION/processors/YOUR_PROCESSOR_ID
UNIFEX_GOOGLE_DOCAI_CREDENTIALS_PATH=/path/to/YOUR_SERVICE_ACCOUNT.json

# LLM API keys for structured extraction
# OpenAI
OPENAI_API_KEY=sk-YOUR_OPENAI_API_KEY

# Anthropic
ANTHROPIC_API_KEY=sk-ant-YOUR_ANTHROPIC_API_KEY

# Google Gemini
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY

# Azure OpenAI (or use UNIFEX_AZURE_DI_ENDPOINT and UNIFEX_AZURE_DI_KEY above)
AZURE_OPENAI_ENDPOINT=https://YOUR_RESOURCE_NAME.openai.azure.com
AZURE_OPENAI_API_KEY=YOUR_AZURE_OPENAI_API_KEY
AZURE_OPENAI_API_VERSION=2024-02-15-preview
