{
	"template": "<YEAR>.<MONTH>.<COUNTER>",
	"version": "2026.2.3",
	"YEAR": 2026,
	"MONTH": 2,
	"COUNTER": 3,
	"targets": {
		"pyproject.toml": {
			"find": "\"<VERSION>\"\\s*#\\s*version",
			"replace": "\"<VERSION>\" # version"
		},
		"simple_oop/version.py": {
			"find": ".+",
			"replace": "program_version = \"<VERSION>\""
		}
	}
}