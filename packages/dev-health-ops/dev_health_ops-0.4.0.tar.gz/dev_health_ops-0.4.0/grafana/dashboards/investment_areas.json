      "targets": [
        {
          "refId": "A",
          "rawQuery": true,
          "rawSql": "SELECT source, target, sum(delivery_units) AS value FROM stats.v_investment_flow_edges WHERE match(ifNull(nullIf(team_id, ''), 'unassigned'), '${team_id:regex}') AND day >= toDate(toDateTime(intDiv($__from, 1000))) AND day < toDate(toDateTime(intDiv($__to, 1000))) GROUP BY source, target ORDER BY value DESC"
        }
      ],
      "options": {
        "mode": "investmentFlow",
        "investmentFlow": {
          "timeWindowDays": 30,
          "valueField": "value",
          "sourceField": "source",
          "targetField": "target",
          "dayField": "day"
        }
      }
    }
