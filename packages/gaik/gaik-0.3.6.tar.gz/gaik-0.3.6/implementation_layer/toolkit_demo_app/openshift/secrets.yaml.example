# Example secrets template - copy to secrets.yaml and fill in values
# DO NOT commit secrets.yaml to version control!

apiVersion: v1
kind: Secret
metadata:
  name: gaik-demo-api-keys
type: Opaque
stringData:
  AZURE_API_KEY: "your-azure-api-key"
  AZURE_ENDPOINT: "https://your-resource.openai.azure.com/"
  AZURE_API_VERSION: "2025-03-01-preview"
---
apiVersion: v1
kind: Secret
metadata:
  name: gaik-demo-supabase
type: Opaque
stringData:
  NEXT_PUBLIC_SUPABASE_URL: "https://your-project.supabase.co"
  NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY: "eyJhbGc..."
  SUPABASE_SECRET_KEY: "eyJhbGc..."
---
apiVersion: v1
kind: Secret
metadata:
  name: gaik-demo-admin
type: Opaque
stringData:
  ADMIN_PASSWORD: "your-admin-password"
