{
  "name": "@pytest-agents/index-agent",
  "version": "0.1.0",
  "description": "Code Indexing AI Agent for pytest-agents",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier --write 'src/**/*.ts' '__tests__/**/*.ts'",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "agent",
    "code-indexing",
    "search",
    "pytest-agents"
  ],
  "author": "Kevin McAllorum",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^25.0.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
