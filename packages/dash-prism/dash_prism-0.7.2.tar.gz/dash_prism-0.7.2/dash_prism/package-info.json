{
  "name": "dash_prism",
  "version": "0.7.2",
  "description": "Prism: Powerful multi-panel workspace manager for Plotly Dash",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/LudwigAJ/dash-prism.git"
  },
  "bugs": {
    "url": "https://github.com/LudwigAJ/dash-prism/issues"
  },
  "homepage": "https://github.com/LudwigAJ/dash-prism",
  "scripts": {
    "venv": "source ./.venv/bin/activate",
    "build:js::dev": "webpack --mode development",
    "build:js": "webpack",
    "build:backends": "poetry run dash-generate-components ./src/ts/components dash_prism -p package-info.json --ignore '^(?!.*Prism).*$'",
    "build": "npm run build:js && npm run build:backends",
    "watch": "npm run build:js::dev -- --watch",
    "dist": "poetry build --outdir dist/",
    "format": "prettier --write src --ignore-path=.prettierignore",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "poetry run pytest tests/ -n auto -v",
    "test:unit": "poetry run pytest tests/ --ignore=tests/integration/ -n auto -v",
    "test:integration": "poetry run pytest tests/integration/ -n auto -v",
    "test:ts": "vitest run",
    "test:ts:watch": "vitest",
    "test:ts:coverage": "vitest run --coverage",
    "coverage": "poetry run pytest tests/ --cov=dash_prism --cov-report=html --cov-report=term"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "autoprefixer": "^10.4.23",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.10.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react": "18.2.0",
    "react-docgen": "^5.4.3",
    "react-dom": "18.2.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.18",
    "ts-loader": "^9.3.1",
    "typescript": "^5.7.2",
    "vitest": "^4.0.17",
    "webpack": "^5.101.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": "16 - 19",
    "react-dom": "16 - 19"
  },
  "author": "ludwig jonsson <developer-prism@protonmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "fast-deep-equal": "^3.1.3",
    "immer": "^11.1.3",
    "lucide-react": "^0.562.0",
    "react-reverse-portal": "^2.3.0",
    "react-split-pane": "^3.0.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
