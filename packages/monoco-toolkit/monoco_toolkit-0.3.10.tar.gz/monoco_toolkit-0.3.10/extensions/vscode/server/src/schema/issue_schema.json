    "dependencies": {
      "default": [],
      "items": {
        "type": "string"
      },
      "title": "Dependencies",
      "type": "array"
    },
    "related": {
      "default": [],
      "items": {
        "type": "string"
      },
      "title": "Related",
      "type": "array"
    },
    "tags": {
      "default": [],
      "items": {
        "type": "string"
      },
      "title": "Tags",
      "type": "array"
    },
    "path": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Path"
