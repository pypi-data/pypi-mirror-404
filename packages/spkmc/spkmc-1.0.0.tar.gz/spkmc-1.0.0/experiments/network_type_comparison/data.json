    "title": "Comparison of Epidemic Dynamics Across Network Types",
    "xlabel": "Time",
    "ylabel": "Proportion of Individuals",
    "legend_position": "upper right",
    "figsize": [
      12,
      8
    ],
    "colors": {
      "S": "#1f77b4",
      "I": "#d62728",
      "R": "#2ca02c"
    },
    "states_to_plot": [
      "I"
    ],
    "dpi": 300,
    "grid": true,
    "grid_alpha": 0.3
  },
  "scenarios": [
