      "samples": 100,
      "num_runs": 10,
      "initial_perc": 0.01,
      "label": "Random-Regular"
    },
    {
      "distribution": "gamma",
      "network_type": "cg",
      "network_size": 10000,
      "shape": 2.0,
      "scale": 0.5,
