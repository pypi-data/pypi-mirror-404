rule "Financial: BANK CHARGE (Counterparty)" {
  match:
    - counterparty ~ "*BANK*CHARGE*"
  set:
    - category = "Review"
    - flags += "BankFee"
}

rule "Financial: BANK CHARGE (Memo)" {
  match:
    - memo ~ "*BANK*CHARGE*"
  set:
    - category = "Review"
    - flags += "BankFee"
}

rule "Financial: CHARGE (Memo)" {
  match:
    - memo ~ "*CHARGE*"
  set:
    - category = "Review"
    - flags += "BankFee"
}

