          }
        ]
      }
    ],
    "PermissionRequest": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "uv run python \"$PROJECT_PATH/.claude/hooks/hook_dispatcher.py\" --type=permission-request"
          }
        ]
      }
    ]
  }
}
