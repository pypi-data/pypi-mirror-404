[run]
branch = True
parallel = True
source =
    tactus
    tactus-ide/backend
omit =
    */__pycache__/*
    tactus/validation/generated/*
    tactus/validation/LuaLexerBase.py
    tactus/validation/LuaParserBase.py
    */tests/*
    */features/*

[report]
show_missing = True
skip_covered = True
precision = 1
fail_under = 100
exclude_lines =
    pragma: no cover
    if __name__ == .__main__.:
    raise NotImplementedError
    @abstractmethod

[html]
directory = coverage_html
