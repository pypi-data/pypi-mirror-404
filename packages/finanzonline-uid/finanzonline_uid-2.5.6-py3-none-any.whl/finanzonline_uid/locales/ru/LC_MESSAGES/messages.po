# CLI messages
msgid "Show full Python traceback on errors"
msgstr "Показывать полную трассировку Python при ошибках"

msgid "Load configuration from a named profile (e.g., 'production', 'test')"
msgstr "Загрузить конфигурацию из именованного профиля (напр., 'production', 'test')"

msgid "Output format (human-readable or JSON)"
msgstr "Формат вывода (читаемый или JSON)"

msgid "Show only a specific configuration section (e.g., 'lib_log_rich')"
msgstr "Показать только определённый раздел конфигурации (напр., 'lib_log_rich')"

msgid "Override profile from root command (e.g., 'production', 'test')"
msgstr "Переопределить профиль из корневой команды (напр., 'production', 'test')"

msgid "Target configuration layer(s) to deploy to (can specify multiple)"
msgstr "Целевой уровень(и) конфигурации для развёртывания (можно указать несколько)"

msgid "Overwrite existing configuration files"
msgstr "Перезаписать существующие файлы конфигурации"

msgid "Interactive mode: prompt for UID to check"
msgstr "Интерактивный режим: запросить UID для проверки"

msgid "Disable email notification (default: email enabled)"
msgstr "Отключить уведомление по электронной почте (по умолчанию: включено)"

msgid "Output format (default: human)"
msgstr "Формат вывода (по умолчанию: читаемый)"

msgid "Email recipient (can specify multiple, uses config default if not specified)"
msgstr "Получатель электронной почты (можно указать несколько, использует значение по умолчанию)"

msgid "profile"
msgstr "профиль"

msgid "Configuration deployed successfully"
msgstr "Конфигурация успешно развёрнута"

msgid "No files were created (all target files already exist)."
msgstr "Файлы не созданы (все целевые файлы уже существуют)."

msgid "All configuration files are already up to date (content unchanged)."
msgstr "Все конфигурационные файлы уже актуальны (содержимое не изменилось)."

msgid "Use --force to overwrite existing configuration files."
msgstr "Используйте --force для перезаписи существующих файлов конфигурации."

msgid "Error"
msgstr "Ошибка"

msgid "Permission denied."
msgstr "Доступ запрещён."

msgid "Hint: System-wide deployment (--target app/host) may require sudo."
msgstr "Подсказка: Системное развёртывание (--target app/host) может потребовать sudo."

msgid "Failed to deploy configuration:"
msgstr "Не удалось развернуть конфигурацию:"

msgid "Enter EU VAT ID to verify"
msgstr "Введите ID НДС ЕС для проверки"

msgid "Error: UID argument is required (or use --interactive)"
msgstr "Ошибка: Требуется аргумент UID (или используйте --interactive)"

msgid "Return code:"
msgstr "Код возврата:"

msgid "This error may be temporary. Try again later."
msgstr "Эта ошибка может быть временной. Повторите попытку позже."

msgid "Configure FinanzOnline credentials in your config file or via environment variables:"
msgstr "Настройте учётные данные FinanzOnline в файле конфигурации или через переменные окружения:"

msgid "8-12 alphanumeric"
msgstr "8-12 буквенно-цифровых"

msgid "5-12 chars"
msgstr "5-12 символов"

msgid "5-128 chars"
msgstr "5-128 символов"

msgid "10-24 alphanumeric"
msgstr "10-24 буквенно-цифровых"

# Retry mode messages
