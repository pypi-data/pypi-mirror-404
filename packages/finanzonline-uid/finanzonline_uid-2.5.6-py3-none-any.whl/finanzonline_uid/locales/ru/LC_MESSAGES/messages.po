msgstr "Попыток выполнено"

msgid "Retry Mode"
msgstr "Режим повтора"

msgid "Attempt"
msgstr "Попытка"

msgid "Cancelled by user after"
msgstr "Отменено пользователем после"

msgid "attempt(s)"
msgstr "попыток"

msgid "Check cancelled by user"
