    "paper_authors",
    "publication_date",
    "publication_venue",
    "module_a",
    "module_b",
    "module_c1",
    "module_c2",
    "module_c3",
    "module_c4",
    "module_c5",
    "module_c6",
    "module_c7",
    "module_c8",
    "module_d",
    "module_e",
    "module_h"
