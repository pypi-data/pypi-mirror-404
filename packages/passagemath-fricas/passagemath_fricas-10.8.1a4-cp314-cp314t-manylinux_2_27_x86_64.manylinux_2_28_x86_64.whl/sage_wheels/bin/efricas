#!/bin/sh -
FRICASEDITOR='emacsclient +$line $name >/dev/null 2>&1'
export FRICASEDITOR
FRICASCMD='/host/sage-manylinux_2_28_x86_64/lib/fricas/target/x86_64-pc-linux-gnu/bin/fricas'
export FRICASCMD
if which emacs; then
  emacs --eval '(progn (server-start) (setq load-path (cons (quote "/host/sage-manylinux_2_28_x86_64/lib/fricas/emacs") load-path)) (setq auto-mode-alist (cons (quote ("\\.fri$" . fricas-mode)) auto-mode-alist)) (require (quote fricas)))' -f fricas $@;
else 
  echo You must have installed emacs to run efricas.;
fi
