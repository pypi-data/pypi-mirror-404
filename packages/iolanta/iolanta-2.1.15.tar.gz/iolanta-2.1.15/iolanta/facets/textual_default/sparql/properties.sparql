SELECT ?property ?object WHERE {
    $this ?property ?object .

    # Exclude non-informative tautologies.
    FILTER (!(
        ?object = $this
        && ?property IN (
            owl:sameAs,
            owl:equivalentClass,
            rdfs:subClassOf
        )
    ))

    FILTER (!( ?property IN (
        rdfs:label
    )))
}
